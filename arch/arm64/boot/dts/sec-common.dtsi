				pinctrl-names = "default", "gpio";
				pinctrl-0 = <&twsi3_pmx_func>;
				pinctrl-1 = <&twsi3_pmx_gpio>;
				i2c-gpio = <&gpio 73 0 &gpio 74 0>;
				status = "okay";
			};

			muici2c: i2c-gpio@8 {
				compatible = "i2c-gpio";
				gpios = <&gpio 30 0 &gpio 29 0>;
				i2c-gpio,delay-us = <3>;
				i2c-gpio,timeout-ms = <100>;
				#address-cells = <1>;
				#size-cells = <0>;
				pinctrl-names = "default";
				pinctrl-0 = <&muic_i2c_pmx>;
				status = "okay";
				muic@25 {
					compatible = "samsung,s2mm001";
					pinctrl-names = "default", "sleep";
					pinctrl-0 = <&muic_gpio_pmx>;
					pinctrl-1 = <&muic_gpio_edge_wakeup>;
					reg = <0x25>;
					interrupt-parent = <&gpio>;
					interrupts = <4 IRQ_TYPE_EDGE_FALLING>;
					connector-gpio = <&gpio 4 0>;
					lpm-qos = <PM_QOS_CPUIDLE_BLOCK_AXI>;
				};
			};

			sensori2c: i2c-gpio@9 {
				compatible = "i2c-gpio";
				gpios = <&gpio 3 0 &gpio 2 0>;
				i2c-gpio,delay-us = <3>;
				i2c-gpio,timeout-ms = <100>;
				#address-cells = <1>;
				#size-cells = <0>;
